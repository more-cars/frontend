{
  "author": "Dennis Gerike",
  "dependencies": {
    "@tailwindcss/cli": "4.1.8",
    "@types/express": "5.0.3",
    "axios": "1.9.0",
    "express": "5.1.0",
    "pug": "3.0.3",
    "tailwindcss": "4.1.8",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "description": "Frontend for the open car database 'More Cars'",
  "devDependencies": {
    "@playwright/test": "1.52.0"
  },
  "engines": {
    "node": ">=22"
  },
  "license": "MIT",
  "name": "@more-cars/rest-api",
  "repository": "https://github.com/more-cars/rest-api",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "local:app:start": "node --watch --env-file=.env -r ts-node/register src/server.ts",
    "local:css:build": "npx @tailwindcss/cli -i src/views/stylesheets/main.css -o ./public/stylesheets/main.css --watch",
    "test:run:gui": "npx playwright test --ui",
    "test:run:cli": "npx playwright test",
    "test:report:show": "npx playwright show-report"
  },
  "version": "0.1.0"
}
