if nodeCollection.length === 0
    div(
        class="max-w-160 m-auto p-4 bg-orange-100 border-l-4 border-orange-500 text-orange-700"
        data-testid="car-model-list-empty"
    )
        p(class="font-bold") No Car Models
        p No car models were found. Either there are none or this is a temporary issue. Please try again later.
else
    section(data-testid="car-model-section")
        ul(class="grid lg:grid-cols-2 gap-x-8 gap-y-2")
            each item in nodeCollection
                include car-model-list-item

    include ../../nodes/pagination
    +pagination('car-models', pagination.page, pagination.total)
